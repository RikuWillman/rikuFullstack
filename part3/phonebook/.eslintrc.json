{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": "off"
  },
  "ignorePatterns": ["dist/", "node_modules/"],
  "globals": {
    "process": "readonly"
  }
}